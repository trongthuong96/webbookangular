{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [
        "/api/*"
      ],
      "cacheConfig": {
        "maxSize": 1000000,
        "timeout": "0u",
        "maxAge": "365d",
        "strategy": "freshness"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index1.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
