{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index1.html",
  "dataGroups": [
    {
      "name": "api-book-cache",
      "urls": [
        "/api/book*",
        "/api/bookreading*"
      ],
      "cacheConfig": {
        "maxSize": 1000000,
        "timeout": "0u",
        "maxAge": "365d",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-genre-cache",
      "urls": [
        "/api/Genre*",
        "/api/crawling*"
      ],
      "cacheConfig": {
        "maxSize": 1000000,
        "timeout": "0u",
        "maxAge": "365d",
        "strategy": "performance"
      }
    },
    {
      "name": "api-chapter-cache",
      "urls": [
        "/api/chapter*"
      ],
      "cacheConfig": {
        "maxSize": 1000000,
        "timeout": "1000u",
        "maxAge": "365d",
        "strategy": "freshness"
      }
    }
  ],
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index1.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/media/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],

  "navigationUrls": [
    "/**",
    "!/**/assets/**"
  ],

  "navigationRequestStrategy": "performance"
  
}
