{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ],
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.+\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.+\\.js)",
      "destination": "/$1"
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/webbook-gui/server/**"
    }
  }
}
