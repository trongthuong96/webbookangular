{
    "version": 2,
    "public": true,
    "routes": [
      { 
        "source": "/(.*)", 
        "destination": "/api"
      }
    ],
    "functions": {
      "api/index.js": {
        "includeFiles": "dist/webbook-gui/server/**"
      }
    },
    "headers": [
      {
        "source": "/favicon.ico",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      },
      {
        "source": "/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31556952, immutable"
          }
        ]
      }
    ]
  }