{
    "version": 2,
    "public": true,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api"
      }
    ],
    "headers": [
      {
        "source": "src/favicon.ico",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      },
      {
        "source": "src/assets/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31556952, immutable"
          }
        ]
      }
    ],
    "functions": {
      "api/index.js": {
        "includeFiles": "dist/webbook-gui/server/**"
      }
    }
  }